      "source": [
        "import pandas as pd\n",
        "\n",
        "def article_views(views: pd.DataFrame) -> pd.DataFrame:\n",
        "    df = views[views['author_id'] == views['viewer_id']][['author_id']].rename(columns = {'author_id' : 'id'}).sort_values(by = 'id')['id'].unique()\n",
        "    return pd.DataFrame({'id' : df})"
      ]
    }
  ]
